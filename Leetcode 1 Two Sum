class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> res;
        unordered_map<int, int> num_map;
        for (int i = 0; i < nums.size(); ++i) {
            if (num_map.find(target-nums[i])!= num_map.end()){
                res.push_back(num_map[target-nums[i]]);
                res.push_back(i);
                return res;
            } else{
                num_map[nums[i]] = i;
            }
        }
        return res;        
    }
};
